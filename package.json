{
  "name": "storyblok",
  "type": "module",
  "version": "3.36.1",
  "packageManager": "pnpm@10.10.0",
  "description": "Storyblok CLI",
  "author": "Alvaro Saburido <hola@alvarosaburido.dev> (https://github.com/alvarosabu/)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/storyblok/storyblok-cli.git"
  },
  "keywords": [
    "storyblok",
    "cli",
    "node",
    "javascript"
  ],
  "main": "./dist/index.mjs",
  "bin": {
    "storyblok": "./dist/index.mjs"
  },
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "unbuild",
    "build:stub": "unbuild --stub",
    "release": "release-it",
    "dev": "pnpm run build:stub && STUB=true node dist/index.mjs",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest",
    "test:ci": "vitest run",
    "test:ui": "vitest --ui",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@inquirer/prompts": "^7.5.0",
    "@topcli/spinner": "^2.1.2",
    "chalk": "^5.4.1",
    "commander": "^13.1.0",
    "dotenv": "^16.5.0",
    "json-schema-to-typescript": "^15.0.4",
    "ohash": "^2.0.11",
    "pathe": "^2.0.3",
    "storyblok-js-client": "^6.10.11"
  },
  "devDependencies": {
    "@storyblok/eslint-config": "^0.3.0",
    "@types/inquirer": "^9.0.7",
    "@types/node": "^22.15.3",
    "@vitest/coverage-v8": "^3.1.2",
    "@vitest/ui": "^3.1.2",
    "eslint": "^9.25.1",
    "memfs": "^4.17.0",
    "msw": "^2.7.5",
    "release-it": "^19.0.2",
    "typescript": "^5.8.3",
    "unbuild": "^3.5.0",
    "vite": "^6.3.4",
    "vitest": "^3.1.2"
  }
}
